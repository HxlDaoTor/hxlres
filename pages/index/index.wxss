.txt-ecd {
  /* background:url("https://www.huasheng188.com/qrcode/bg/wenhao.jpg") no-repeat right center; */
  /* background-size: 25rpx 25rpx; */
  padding-right: 10rpx;
  font-size: 24rpx;
  color:#999;
}
.topBaner{
  background-color: #ff5555;
   background: -webkit-linear-gradient(#ff5555,#fff);
color:#fff;
display: flex;
justify-content: center;
align-content: center;
padding:0 0 40px;
}
.topBaner .money{
  flex: 1;
  display: flex;
  text-align: center;
  align-content: center;
  justify-content: center;
  flex-direction: column;
  position: relative;
  top:-10px;
}
.topBaner .money:last-child::after{
 content: "";
 width: 1px;
 position: absolute;
 background:rgba(255,255,255,.2);
 top: 20px;
 bottom: 20px;
 left: 0px;
}
.topBaner .money view{
  font-size: 12px;
}
.topBaner .money view text{
  font-size: 28px;
}
.topBaner .money .iconfont{
  font-size: 12px;
  background: rgba(255,255,255,.2);
  
  text-align:center;
   margin:0 auto;
  padding:4px 4px 4px 8px;
  border-radius: 20px;
  color:#fff;
}
.iconBtn{

  display: flex;
  justify-content: center;
  align-items: center;
padding:6px 0;
margin:-30px 15px 10px;
border-radius: 6px;
/* background: #fff;
box-shadow:  0 0 12px rgba(255,85,85,.1); */
}
.iconBtn view{
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  font-size: 10px;
  color:#4d4d4d;
}
.iconBtn image{
  width: 44px;
  height: 44px;
  vertical-align: middle;
}
.iconBtn .signIn text{
  color:#ababab;
  position: relative;
}
.iconBtn .signIn .issig{
  color:#4d4d4d;
}
.iconBtn .signIn .issig::after{
  content: '.';
  position: absolute;
  right: -10px;
  top:-26px;
  color:#ff5555;
  font-size: 30px;

}
.title{
  font-size: 14px;
  color:#4d4d4d;
  padding:0 14px;
  font-weight: bold;
  text-align:center;
  line-height: 30px;
  background: #fff;
}
.listgoods{
padding: 0 15px 10px;

}
.listgoods .goods{
  /* height: 160px; */
  margin-bottom: 10px;
  background:#fff;
  border-radius: 6px;
  box-shadow: 0 2px 10px rgba(95, 56, 56, 0.1);
  display: flex;
  justify-content: center;
align-items: center;
  flex-direction: column;
  padding: 10px;
    position:relative;
}
.listgoods .goods .titl{
    text-align: left;
  width: 100%;
  font-size: 14px;
  color:#4d4d4d;
  line-height: 20px;
  margin-top:4px;
}
.listgoods .goods .titl text{
  color:#808080;
}
.listgoods .goods text.tipp{
    display: block;
  text-align: left;
  width: 100%;
  color:#ababab;
  font-size: 12px;
  margin-top:4px;
}
.listgoods .goods image{
  width: 160px;
  height: 100px;
}
.listgoods .goods .sponsorship{
  position:absolute;
  right: 0;
  top:10px;
  padding:4px 10px;
  color:#fff;
  font-size: 10px;
  background:rgba(0,0,0,.4);
  border-radius: 20px 0 0 20px;
}
.listgoods .goods .isParticipate{
   position:absolute;
   left: 0;
  top:10px;
  padding:2px 10px;
  color:#fff;
  font-size: 10px;
  background:#15c383;
  border-radius: 0 15px 0 0;
}

.fl_bj{
  text-align: center;
  background:url("data:image/png;base64,") no-repeat  center center;
  background-size: 100%;
  margin-bottom:10px;
}
.listdgs{
 padding: 0 15px 60px;
}
.listdgs navigator{
