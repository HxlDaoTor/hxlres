 justify-content: space-between;
 align-items: center;
 background:#fefcfc;
   border-radius: 6px;
  box-shadow: 0 2px 6px rgba(95, 56, 56, 0.1);
  font-size: 14px;
  color:#4d4d4d;
  margin-bottom: 10px;
   overflow: hidden;
}
.listdgs navigator .imgsve{
  width: 40%;
