.bdy{
  min-height: 100vh;
  background:#f5f5f5;
}
.fled{
display:flex;
justify-content:center;
align-items:center;
flex-direction:column;
padding:15px 0;
}
.fled .tip{
  color:#4d4d4d;
  font-size: 14px;
}
.headPor{
  display:flex;
}

.headPor{
  display:flex;
  flex-wrap: wrap;
  /* width:80%; */
  padding:15px 30px 4px;
  justify-content: space-around;
}
.headPor view{
  width: 32px;
  height:0;
}
.headPor image{
  width: 30px;
  height:30px;
  margin-bottom: 3px;
  margin-right: 2px;
}
.headPor image:last-child{
  margin-right: 0;
}
.more{
  width: 100%;
  text-align: center;
}
.more text{
  font-size: 14px;
  color:#458aca;
}