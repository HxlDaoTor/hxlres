.tab-bar {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 48px;
  background: white;
  display: flex;
  padding-bottom: env(safe-area-inset-bottom);
  justify-content: content;
  align-content: center;
  border-top:1px solid #f5f5f5;
}

.tab-bar-border {
  background-color: rgba(0, 0, 0, 0.1);
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 1px;
  transform: scaleY(0.5);
}

.tab-bar-item {
  flex: 1;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}

.tab-bar-item image {
  width: 18px;
  height: 18px;
}
.tab-bar-item image.jia{
  width:40px;
  height:40px;
  margin-top:-20px;
 position: absolute;
 z-index: 1;
}
.tab-bar-item view {
  font-size: 10px;
  margin-top:2px;
}
.faqi{
  position: relative;
}
.faqi::before{
  content: '';
  position: absolute;
  width: 40px;
  height:40px;
  border-radius: 50%;
  background:#fff;
  border:1px solid #f5f5f5;
  top:-18px;

}
.faqi::after{
  content: "";
  background:#fff;
  position: absolute;
  width:42px;
  height:30px;
  top: 0;

}
.faqi  view{
  margin-top:20px;
}
